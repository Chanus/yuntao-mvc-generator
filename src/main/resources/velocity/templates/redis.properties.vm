# Redis Setting
# Redis 默认有16个库，序号是0-15，默认是选中的是0号数据库
spring.redis.database = 0
# Redis 服务器地址
spring.redis.host = ${projectVO.redis.host}
# Redis 服务器连接端口，默认是6379
spring.redis.port = ${projectVO.redis.port}
# Redis 服务器连接密码（默认为空）
spring.redis.password = ${projectVO.redis.password}
# Redis 服务器连接超时时间
spring.redis.expiration = 5000
# 连接池中的最大连接数
spring.redis.pool.maxTotal = 600
# 连接池中的最大空闲连接数
spring.redis.pool.maxIdle = 300
# 连接池中的最小空闲连接数
spring.redis.pool.minIdle = 100
# 连接池最大阻塞等待时间（使用负值表示没有限制）
spring.redis.pool.maxWaitMillis = 1500
# 连接超时时间（毫秒）
spring.redis.timeout = 2000