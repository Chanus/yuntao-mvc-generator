##### 系统配置 ####

# 系统名称
system.name = ${projectVO.projectName}

# 系统版本
system.version = ${projectVO.version}

# 系统声明
system.copyright = ${projectVO.copyright}

# 自动生成代码配置信息(暂时放在这里，后期考虑单独保存到一个配置文件，或是保存到数据库中)

# 代码作者
code.generation.author = ${projectVO.author}
# 代码版本号
code.generation.since = ${projectVO.version}
# 项目是否将后端分成两个模块，1 表示分成两个模块，0 表示单模块
code.generation.multi = ${multiModule}
# 生成服务类的包名
code.generation.server.package = ${apiPackage}
# 生成控制器的包名
code.generation.controller.package = ${webPackage}
# 表前缀，生成类名时会去除该前缀，例如，sys_log 生成 Log.java。如表不存在前缀可不配置
# code.generation.tablePrefix =
# 生成类名时是否自动去除表前缀，0-否，1-是。此项也可不配置，即不自动去除前缀
code.generation.autoRemovePrefix = 0